{
  "name": "autoimport",
  "main": "./lib/autoimport",
  "version": "0.0.0",
  "description": "A short description of your package",
  "keywords": [],
  "activationCommands": {},
  "repository": "https://github.com/atom/autoimport",
  "license": "MIT",
  "scripts": {
    "prepublish": "npm run build",
    "watch": "babel --watch src/ -d lib/",
    "build": "npm run build:clean && npm run build:babel",
    "build:clean": "rimraf lib",
    "build:babel": "babel src/ -d lib/",
    "build:flow": "flow-copy-source -v src lib"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "@babel/generator": "^7.0.0-beta.38",
    "@babel/types": "^7.0.0-beta.38",
    "babylon": "^6.18.0",
    "camelcase": "^4.1.0",
    "find-package": "^1.0.0",
    "flow-bin": "^0.63.1",
    "glob": "^7.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.38",
    "@babel/core": "^7.0.0-beta.38",
    "@babel/preset-env": "^7.0.0-beta.38",
    "@babel/preset-flow": "^7.0.0-beta.38",
    "rimraf": "^2.6.2"
  },
  "babel": {
    "plugins": ["./dev/use-babel-plugin"],
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "electron": "1.6.15"
          },
          "modules": false
        }
      ],
      "@babel/preset-flow"
    ]
  }
}
